public class Empleado
//Clase padre
//@author Elizabeth
{
    //ATRIBUTOS
    //@param private nombre
    private String nombre;

    //METODO CONSTRUCTOR

    public Empleado()
    {
	//@return nombre
	this.nombre = Elizabeth;
    }

    public Empleado(String nombre)
    {
	//@param nombre
	this.nombre = nombre;
    }

    //METODOS ESPECIALES

    public void setNombre(String nombre)
    {
	this.nombre = nombre;
    }

    public String getNombre()
    {
	return this.nombre;
    }

    
	
}
